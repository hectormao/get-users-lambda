{
  "name": "get-users-lambda",
  "version": "1.0.0",
  "description": "Lambda to get all users, aplying filter by genre",
  "main": "dist/handler.js",
  "scripts": {
    "build": "npx tsc -p .",
    "clean": "rm -Rf *.zip",
    "main": "export AWS_PROFILE=personal && export DB_NAME=listado_usuarios && export SECRET_ID=cursolambda && ts-node ./src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "npm run clean && npm run build && npx npm-pack-zip"
  },
  "author": "hectormao.gonzalez@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.106",
    "@types/mariasql": "0.1.31",
    "@types/node": "^18.8.4",
    "npm-pack-zip": "1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1231.0",
    "http-status": "1.5.3",
    "inversify": "6.0.1",
    "mariadb": "3.0.1",
    "reflect-metadata": "0.1.13",
    "sqlstring": "2.3.3"
  },
  "bundledDependencies": [
    "aws-sdk",
    "http-status",
    "inversify",
    "reflect-metadata",
    "mariadb",
    "sqlstring"
  ]
